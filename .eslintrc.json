{
    "extends": [
        "plugin:v3xlabs/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "ignorePatterns": [
        "!**/*"
    ],
    "plugins": [
        "v3xlabs"
    ],
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "sonarjs/cognitive-complexity": ["warn", 20]
    }
}